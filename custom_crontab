# Check-in everyday at 07:30 to 08:30
30 07 * * * /home/teamdev/FaceRec/testing_app/run_main.sh -d 3600 -t IN > /home/teamdev/FaceRec/testing_app/logs/check-in.log 2>&1

# Check-out from Mon to Fri at 17:20 to 18:20
20 17 * * MON-FRI /home/teamdev/FaceRec/testing_app/run_main.sh -d 3600 -t OUT > /home/teamdev/FaceRec/testing_app/logs/check-out.log 2>&1

# Check-out on Sat at 11:55
55 11 * * SAT /home/teamdev/FaceRec/testing_app/run_main.sh -d 1800 -t OUT > /home/teamdev/FaceRec/testing_app/logs/check-out.log 2>&1

